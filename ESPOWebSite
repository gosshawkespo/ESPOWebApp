                if ($(this)[0].localName === 'a') {
                    $(this).attr('title',$(this)[0].innerText);
                }
            });
        }
    });
</script>
<script>
    require(['jquery'],function($){
        var form = $('#newsletter-validate-detail');
        form.submit(function(e) {
            var loadingMessage = $('#loading-message');
            if(form.validation('isValid')){
                var email = $("#newsletter").val();
                var url = form.attr('action');
                e.preventDefault();
                try{
                    loadingMessage.removeClass('success');
                    loadingMessage.removeClass('error');
                    $.ajax({
                        url: url,
                        dataType: 'json',
                        type: 'POST',
                        data: {email: email},
                        success: function (data){
                            if(data.status != "ERROR"){
                                $('#newsletter').val('').trigger('change');
                                loadingMessage.removeClass('error');
                                loadingMessage.addClass('success');
                            }else{
                                loadingMessage.removeClass('success');
                                loadingMessage.addClass('error');
                            }
                            loadingMessage.html(data.msg);
                        },
                    });
                } catch (e){
                    loadingMessage.removeClass('success');
                    loadingMessage.addClass('error');
                    loadingMessage.html(e.message);
                }
            } else {
                loadingMessage.html('');
            }
        });
    })
</script>
<script type="text/javascript">
    require(['jquery', 'matchMedia'], function($, mediaCheck) {
            var detailsTabs = $('#newsletter');
            mediaCheck({
                media: '(min-width: 768px)',
                entry: function () {
                    detailsTabs.attr("placeholder", "Enter your email here");
                },
                exit: function () {
                    detailsTabs.attr("placeholder", "Get our latest updates");
                }
            });
        })
</script>
