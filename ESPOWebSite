<script type="text/javascript">
    require(['jquery'],function($){
        var isIE11 = !!navigator.userAgent.match(/Trident.*rv\:11\./);
        setTimeout(function(){
            $('a.lazy').css('opacity', '1');
            $('div.lazy').css('opacity', '1');
        }, 200);
        var lazyLoadingImage = function () {
            $('img.lazy').unveil(100, function() {
                $(this).load(function() {
                    this.style.opacity = 1;
                });
            });
        };

        var showLoadingImage = function() {
            $('img.lazy').each(function() {
                $(this).attr('src',$(this).attr('data-src'));
                $(this).css('opacity', '1');
            });
        }

        setTimeout(function(){
            if (isIE11) {
                showLoadingImage();
            } else {
                lazyLoadingImage();
            }
        }, 1000);


        if ($('.catalog-category-view').length > 0) {
            $(document).on('click','.bss_checkbox',function(){
                showLoadingImage();
            });
            $(document).on('click','.sorter-options',function(){
                showLoadingImage();
            });
            $(document).on('click','.sorter-action',function(){
                showLoadingImage();
            });
            $(document).on('click','.limiter-options',function(){
                showLoadingImage();
            });
        }

        $(document).on('click','.ui-tabs-anchor',function(){
            lazyLoadingImage();
        });

        $(document).on('change', '.limiter-options', function(){
            lazyLoadingImage();
        });

        $(document).on("click", ".menu-dropdown-icon", function(){
            lazyLoadingImage();
        });

    });
</script>
<script type="text/javascript">
    var animation_time = '0.7';
    requirejs(['jquery'], function ($) {
        var nav = $('.horizontal-menu');
        if (nav.length) {
            var stickyHeaderTop = nav.offset().top;
            $(window).scroll(function () {
                if ($(window).width() >= 768) {
                    if ($(window).scrollTop() > stickyHeaderTop) {

                        if ($('.horizontal-menu .stickymenu').hasClass('vertical-right')) {
                            var outerWidth = $('.section-items.nav-sections-items').width();
                            var innerWidth = $('.menu-container.horizontal-menu').width();
                            var rightMargin = ((outerWidth - innerWidth) / 2) + 'px';
                            $('.horizontal-menu .stickymenu').css({position: 'fixed', top: '0px', right: rightMargin});
                        } else {
                            $('.horizontal-menu .stickymenu').css({position: 'fixed', top: '0px'});
                        }

                        $('.stickyalias').css('display', 'block');
                    } else {
                        $('.horizontal-menu .stickymenu').css({position: 'static', top: '0px'});
                        $('.stickyalias').css('display', 'none');
                    }
                }
            });

            $('.section-item-content .menu-container.horizontal-menu .menu > ul li.dropdown').each(function (e) {
                $(this).children('a').after('<span class="plus"></span>');
            });

            $('.section-item-content .menu-container.horizontal-menu .menu > ul li.dropdown span.plus').click(function (e) {
                $(this).siblings('a').toggleClass('active');
                $(this).toggleClass('active').siblings('ul').slideToggle('500');
            });
        }

        $('.menu.horizontal > ul').on('mouseover', function () {
            if (!$('body').hasClass('mega-menu-active')) {
                $('body').addClass('mega-menu-active');
            }
        });
        $('.menu.horizontal > ul').on('mouseleave', function () {
            if ($('body').hasClass('mega-menu-active')) {
                $('body').removeClass('mega-menu-active');
            }
        });
    });
</script>
<script type="text/javascript">
    require(['jquery'],function($){
        var tabsNavigation = $('.tabs-navigation');
        if (tabsNavigation.length > 0) {
