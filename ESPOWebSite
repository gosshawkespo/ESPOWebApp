<script type="text/javascript">
    require(['jquery'] , function($) {
        if ($('.authorization-link').length > 0) {
            $('.header-account').addClass('active');
            var isLoggedIn = $('.authorization-link > a').attr('href').indexOf('/login');
            if (isLoggedIn < 0) {
                $('.header-panel-content').addClass('active');
                $('.header-account-content').removeClass('active');
            } else {
                $('.header-panel-content').removeClass('active');  
                $('.header-account-content').addClass('active');
            }
        } else {
            $('.header-account').removeClass('active');
        }

        var headerAccountMobileLink = $('.header-account-mobile-link');
        var headerAccountMobileContent = $('.header-account-mobile-content');
        if (headerAccountMobileLink && headerAccountMobileLink.length > 0) {
            $(document).on('click', '.header-account-mobile-link', function(){
                if ($('.header-account-mobile-link').hasClass('active')) {
                    $('.header-account-mobile-link').removeClass('active');
                    $('.header-account-mobile-content').removeClass('active');
                } else {
                    $('.header-account-mobile-link').addClass('active');
                    $('.header-account-mobile-content').addClass('active');
                }
            });
        }

        $(document).click(function(event){    
            if(!$(event.target).is('.header-account-mobile-link')) {
                $('.header-account-mobile-link').removeClass('active');
                $('.header-account-mobile-content').removeClass('active');
            }
        });
        
        $('#contentarea').attr('href', '/');
        $('#contentarea').text('Content Area');
    }); 
</script>
